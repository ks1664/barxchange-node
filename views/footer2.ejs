<script>
    function checksession() {
        // alert();
        let formdata = new FormData();
        formdata.append("action", "checksession");
        var xml = new XMLHttpRequest();
        xml.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                var session = this.responseText;
                // alert(session);
                // console.log(ar);
                if (session == "session_empty") {
                    window.location.href = "/users/stafflogin";
                } else {
                    document.getElementById("staffsession").innerHTML ="Hi, " +session;
                    document.getElementById("staffsession2").innerHTML ="Hi, " +session;
                }
            } else {
                document.getElementById("staffsession").innerHTML = "<span class='spinner-border'></span>";
                document.getElementById("staffsession2").innerHTML = "<span class='spinner-border'></span>";
            }
        };
        xml.open("POST", "./staffaction", true);
        xml.send(formdata);
    }

</script>
