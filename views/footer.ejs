<script>
    function checksession() {
        // alert();
        let formdata = new FormData();
        formdata.append("action", "checksession");
        var xml = new XMLHttpRequest();
        xml.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                var session = this.responseText;
                // alert(session);
                // console.log(ar);
                if (session == "session_empty") {
                    window.location.href = "/admin/adminlogin";
                } else {
                    document.getElementById("adminsession").innerHTML = session;
                }
            } else {
                document.getElementById("adminsession").innerHTML = "<span class='spinner-border'></span>";
            }
        };
        xml.open("POST", "./adminaction", true);
        xml.send(formdata);
    }
</script>
